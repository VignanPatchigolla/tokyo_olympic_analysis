{
	"name": "s3_csv_genric",
	"properties": {
		"linkedServiceName": {
			"referenceName": "aws_s3_ls",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"file_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AmazonS3Location",
				"bucketName": "tokyoolympicdatasets",
				"folderPath": "datasets",
				"fileName": {
					"value": "@dataset().file_name",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "PersonName",
				"type": "String"
			},
			{
				"name": "Country",
				"type": "String"
			},
			{
				"name": "Discipline",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}